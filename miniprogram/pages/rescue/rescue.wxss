/* miniprogram/pages/rescue/rescue.wxss */
swiper{
  width: 100%;
  height: 100rpx;
}
swiper-item text{
  font-size: 40rpx;
  text-align: center;
  line-height: 100rpx;
  color:red;
  margin-left:20rpx;
}
.user{
  border-top:1rpx solid #D5D5D5;
  border-bottom:1rpx solid #D5D5D5;
  margin:20rpx;
}
.wx_information{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 10rpx;
}
.wx_information image{
  width: 70rpx;
  height: 70rpx;
  border-radius: 100%;
  margin:3rpx;
}
.wx_information .name{
  margin-left:20rpx ;
  margin-top:20rpx ;
}
.box{
 background-color:#ffffff;
 color: #000000;
}
 .address{
  margin:20rpx;
  letter-spacing: 5rpx;
  font-size: 35rpx;
}